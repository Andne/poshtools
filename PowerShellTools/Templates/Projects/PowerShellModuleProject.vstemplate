<VSTemplate Type="Project" Version="2.0.0"
    xmlns="http://schemas.microsoft.com/developer/vstemplate/2005">
    <TemplateData>
        <Name>PowerShell Module Project</Name>
        <Description>A PowerShell module project.</Description>
        <Icon>PowerShellModuleProject.ico</Icon>
        <ProjectType>PowerShell</ProjectType>
        <DefaultName>PowerShellModuleProject</DefaultName>
        <ProvideDefaultName>true</ProvideDefaultName>
    </TemplateData>
    <TemplateContent>
        <Project File="PowerShellModuleProject.pssproj">
            <ProjectItem>Module.psd1</ProjectItem>
            <ProjectItem OpenInEditor="true">Module.psm1</ProjectItem>
            <ProjectItem>Module.Tests.ps1</ProjectItem>
        </Project>
    </TemplateContent>
</VSTemplate>